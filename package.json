{
  "name": "nextjs-ts-tailwind-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint './**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint './**/*.{ts,tsx,js,jsx}' --fix",
    "prettier": "prettier --check './**/*.{ts,tsx,js,jsx,scss,css}'",
    "prettier:write": "prettier --write './**/*.{ts,tsx,js,jsx,scss,css}'",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "e2e:ci": "start-server-and-test start http://localhost:3000 cypress:headless",
    "analyse": "ANALYZE=true yarn build"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.2.11",
    "autoprefixer": "^10.4.14",
    "next": "14.2.4",
    "postcss": "^8.4.47",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sass": "^1.78.0",
    "tailwindcss": "^3.4.11"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "22.1.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "cypress": "^13.14.2",
    "eslint": "8.46.0",
    "eslint-config-next": "^14.2.4",
    "eslint-plugin-react": "^7.33.1",
    "prettier": "3.3.3",
    "start-server-and-test": "^2.0.7",
    "typescript": "5.5.4"
  },
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ]
}
